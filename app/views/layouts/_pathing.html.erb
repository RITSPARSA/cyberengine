<ul class="breadcrumb">
<% unless @member.nil? || @member.id.nil? %>
  <li class='path'>
    <%= link_to member_path(@member) do %><i class="icon-arrow-right"></i><span><%= @member.username %></span><% end %>
  </li>
<% end %>
<% unless @team.nil? || @team.id.nil? %>
  <li class='path'>
    <%= link_to team_path(@team) do %><i class="icon-arrow-right"></i><span><%= @team.alias %></span><% end %>
  </li>
<% end %>
<% unless @server.nil? || @server.id.nil? %>
  <li class='path'>
    <%= link_to team_server_path(@team,@server) do %><i class="icon-arrow-right"></i><span><%= @server.name %></span><% end %>
  </li>
<% end %>
<% unless @service.nil? || @service.id.nil? %>
  <li class='path'>
    <%= link_to team_server_service_path(@team,@server,@service) do %><i class="icon-arrow-right"></i><span><%= @service.name %></span><% end %>
  </li>
<% end %>
<% unless @check.nil? || @check.id.nil? %>
  <li class='path'>
    <%= link_to team_server_service_check_path(@team,@server,@service,@check) do %><i class="icon-arrow-right"></i><span><%= @check.round %></span><% end %>
  </li>
<% end %>
<% unless @user.nil? || @user.id.nil? %>
  <li class='path'>
    <%= link_to team_server_service_user_path(@team,@server,@service,@user) do %><i class="icon-arrow-right"></i><span><%= @user.username %></span><% end %>
  </li>
<% end %>
<% unless @property.nil? || @property.id.nil? %>
  <li class='path'>
    <%= link_to team_server_service_user_path(@team,@server,@service,@property) do %><i class="icon-arrow-right"></i><span><%= @property.category %></span><% end %>
  </li>
<% end %>
</ul>